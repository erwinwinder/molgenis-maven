    	"MolgenisFile" -> "Identifiable"
    	"RuntimeProperty" -> "Identifiable"

/*one to many 'xref' foreign key relationships*/
        edge [
                arrowhead = "open"
                arrowsize = 0.6
                color = "black"
        ]
        
        
/*many to many 'mref' foreign key relationships*/
