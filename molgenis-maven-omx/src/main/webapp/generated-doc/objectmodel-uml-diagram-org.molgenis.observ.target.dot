/*interface relationships*/
        edge [
                arrowhead = "empty"
                color = "#808080"
        ]

/*inheritance relationships*/
        edge [
                arrowhead = "empty"
                color = "black"
        ]
        
        "Species" -> "OntologyTerm"
        "Individual" -> "ObservationTarget"
        "Panel" -> "ObservationTarget"
        "Accession" -> "OntologyTerm"

/*one to many 'xref' foreign key relationships*/
        edge [
                arrowhead = "open"
                arrowsize = 0.6
        ]
		"Individual" -> "Individual" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Individual" -> "Individual" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Panel" -> "OntologyTerm" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Panel" -> "Species" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"PanelSource" -> "Panel" [
			headlabel = "1"
			taillabel = "*"
		]
		"PanelSource" -> "Panel" [
			headlabel = "1"
			taillabel = "*"
		]
		"OntologyTerm" -> "Ontology" [
			headlabel = "0..1"
			taillabel = "*"
		]
        
        
/*many to many 'mref' foreign key relationships*/
        edge [
             arrowtail = "open"
             color = "black"
             headlabel = "*"
             taillabel = "*"
             arrowsize = 0.6
        ]
