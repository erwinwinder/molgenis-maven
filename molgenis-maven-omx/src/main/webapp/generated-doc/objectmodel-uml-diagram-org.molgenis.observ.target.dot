                	    style = "filled"
                	    fillcolor =  "white"
			        	fontname = "Arial"
			        	fontcolor = "black"
			        	color = "black"
                
                        label = "{PanelSource\n implements Autoid|id : int*\lcurrentPanel : xref-&gt;panel*\lsourcePanel : xref-&gt;panel*\lnumberOfIndividuals : int\lselectionCriteria : text*\l}"
                ]

               "Ontology" [
